<div class="page-wrapper">

  <h1>Bookings#New</h1>

  <div class="image-cover" style="background-image:  url(<%= image_path 'eventspaces/exp1' %>);">
  </div>

  <%= simple_form_for([@space, @booking]) do |f| %>
    <%= f.input :start_date, as: :date, html5: true %>
    <%= f.input :end_date, as: :date, html5: true %>
    <%= f.submit %>
  <% end %>

  <%= link_to 'Back to Spaces#Index', spaces_path %>
</div>
