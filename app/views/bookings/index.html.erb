<h1>Bookings#Index</h1>

<% if @bookings.any? %>
  <% @bookings.each do |booking| %>
    <p>start_date: <%= booking.start_date %></p>
    <p>end_date: <%= booking.end_date %></p>
    <p>status: <%= booking.status %></p>
    <p>space_id: <%= booking.space_id %></p>
    <p><%= link_to 'Back to space', space_path(@space) %></p>
    <br>
  <% end %>
<% else %>
  <p>Sorry, there's no booking for this space.</p>
<% end %>




